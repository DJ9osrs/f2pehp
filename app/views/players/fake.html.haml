- @title = "F2P Hall of Shame" 
= link_to image_tag("f2pwiki_500.png"), players_path

%p#notice= notice

= content_tag(:div, nil, class: "container", style: "width: 820px;") do 
  = content_tag(:div, nil, class: ["bar", "ranks"]) do
    = link_to "Hiscores", ranks_path
  = content_tag(:div, nil, class: ["bar", "fake"]) do 
    = link_to "Current Top", tracking_path
  = content_tag(:div, nil, class: ["bar", "ehp"]) do 
    = link_to "EHP Rates", ehp_path
  = content_tag(:div, nil, class: ["bar", "calcs"]) do 
    = content_tag(:div, nil, class: ["container", "calcs"], style: "margin-bottom: 0px; padding: 0px;", id: "calcs") do 
      = "Calculators"
    = content_tag(:div, nil, class: ["dropdown", "container"], id: "calcs-list", remote: true) do
      = link_to "Combat DPS", dps_path
      = link_to "Combat Lvl", combat_path
      = link_to "GP/XP, Alch", gpxp_path
  = content_tag(:div, nil, class: ["bar", "links"]) do 
    = content_tag(:div, nil, class: ["container", "links"], style: "margin-bottom: 0px; padding: 0px;", id: "links") do 
      = "Links"
    = content_tag(:div, nil, class: ["dropdown", "container"], id: "links-list", remote: true) do
      = link_to "Links", links_path
      = link_to "Hall of Shame", fake_path
  = content_tag(:div, nil, class: ["bar", "changelog"]) do 
    = link_to "Changelog", changelog_path
  = content_tag(:div, nil, class: ["bar", "donate"]) do 
    = link_to "About Us/Donate", donate_path
    
= content_tag(:div, nil, style: "margin: auto; width: 225px; padding: 5px;") do 
  %h1 F2P Hall of Shame
  = "special thanks to Pawz for his confirmed fake f2p list"

= content_tag(:div, nil, style: "margin: auto; max-width: 220px; overflow-x: auto; margin-top: 10px; margin-bottom: 5px; color: none; background-color: transparent; border: none") do 
  = content_tag(:div, nil, class: ["container", "left"], style: "width: 100px;") do 
    = content_tag(:div, nil, class: "container", id: "headerHiscores", style: "margin: auto; width: 100px; height: 9px; margin-bottom: 0px;") 
    = content_tag(:table, nil, style: "width: 100px; min-width: 100px; padding: 5px; margin-bottom: 0px;") do 
      %thead
        %tr
          %th Fake F2P
      %tbody
        - F2POSRSRanks::Application.config.fakes.each do |player|
          %tr
            %td{:align => "center"}= player
    = content_tag(:div, nil, class: "container", id: "footerHiscores", style: "margin: auto; width: 100px; height: 9px; margin-bottom: 0px;") 

  = content_tag(:div, nil, class: ["container", "left"], style: "margin-left: 20px; width: 100px;") do 
    = content_tag(:div, nil, class: "container", id: "headerHiscores", style: "margin: auto; width: 100px; height: 9px; margin-bottom: 0px;") 
    = content_tag(:table, nil, style: "margin-left: auto; float: right; width: 100px; min-width: 100px; padding: 5px; margin-bottom: 0px;") do 
      %thead
        %tr
          %th Banned
      %tbody
        - F2POSRSRanks::Application.config.banned.each do |player|
          %tr
            %td{:align => "center"}= player
    = content_tag(:div, nil, class: "container", id: "footerHiscores", style: "margin: auto; width: 100px; height: 9px; margin-bottom: 0px;") 
