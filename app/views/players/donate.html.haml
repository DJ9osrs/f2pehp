- @title = "About Us" 
= link_to image_tag("f2pwiki_500.png"), players_path

%p#notice= notice
  
= content_tag(:div, nil, class: "container", style: "width: 820px;") do 
  = content_tag(:div, nil, class: ["bar", "ranks"]) do
    = link_to "Hiscores", ranks_path
  = content_tag(:div, nil, class: ["bar", "ehp"]) do 
    = link_to "EHP Rates", ehp_path
  = content_tag(:div, nil, class: ["bar", "calcs"]) do 
    = content_tag(:div, nil, class: ["container", "calcs"], style: "margin-bottom: 0px; padding: 0px;", id: "calcs") do 
      = "Calculators"
    = content_tag(:div, nil, class: ["dropdown", "container"], id: "calcs-list", remote: true) do
      = link_to "Combat DPS", dps_path
      = link_to "Combat Lvl", combat_path
      = link_to "GP/XP, Alch", gpxp_path
  = content_tag(:div, nil, class: ["bar", "fake"]) do 
    = link_to "Hall of Shame", fake_path
  = content_tag(:div, nil, class: ["bar", "links"]) do 
    = link_to "Links", links_path
  = content_tag(:div, nil, class: ["bar", "changelog"]) do 
    = link_to "Changelog", changelog_path
  = content_tag(:div, nil, class: ["bar", "donate", "selected-link"]) do 
    = "About Us/Donate"

= content_tag(:div, nil, class: "container", style: "width: 740px;") do 
  = content_tag(:div, nil, class: "left", style: "margin-left: 30px; text-align: left; width: 430px; padding: 10px;") do 
    %h1{:align => "center"} About Us
    = content_tag(:div, nil, style: "text-align:center; margin: auto; width: 420px; margin-top: 0px; margin-bottom: 0px; color: none; background-color: transparent; border: none") do 
      = "This is a fan-made passion project started in October 2017. It will always be 100% ad-free and non-profit, and is constantly updated with the help of the F2P OSRS community. We work with respected F2P individuals to bring forth EHP balances and meta changes. We also work on new features and additions all the time, and we welcome any suggestions." 
      = "Special thanks to "
      = link_to "Bargan", "players/Bargan"
      = " and "
      = link_to "Obor", "players/Obor"
      = " for their keen eye on bug fixes and suggestions."
    %h3{:align => "center"} Please consider supporting this free service.
    = content_tag(:div, nil, style: "text-align:center; margin: auto; width: 420px; margin-top: 0px; margin-bottom: 0px; color: none; background-color: transparent; border: none") do 
      = "Money is literally our least important concern, as we wish only to serve the F2P OSRS community. However, hosting and server fees add up, so all donations are greatly appreciated."
    %h3{:align => "center"} You will support:
    &nbsp;
    = "- consistent site improvements requiring countless, unpaid hours"
    %br
    &nbsp;
    = "- hosting fees and server/database costs"
    %br
    &nbsp;
    = "- new features and more comprehensive/interesting F2P data"
    
    %h3{:align => "center"} You will receive:
    &nbsp;
    = "- your name on the F2P Supporters list and a special donator flair"
    %br
    &nbsp;
    = "- text on your F2P account's page or a custom name styling/flair: $5"
    %br
    &nbsp;
    = "- your text displayed in the Message Of The Day: $1/day"
    %br
    &nbsp;&nbsp;&nbsp;
    = "(no excessively offensive language, please)"
    %br
    %br
    = content_tag(:div, nil, class: "container", style: "width: 100px;") do 
      <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
      <input type="hidden" name="cmd" value="_s-xclick">
      <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
      ">
      <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donate_SM.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!">
      <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
      </form>
    = content_tag(:div, nil, class: "container", style: "text-align: center; width: 430px;") do 
      = "please leave your F2P account name and your request in the note, or coordinate with us at f2posrsranks@gmail.com"
      
  = content_tag(:div, nil, class: ["container", "left"], style: "margin-left: 10px; width: 240px;") do 
    = content_tag(:div, nil, style: "text-align:center; width:220px; padding: 5px; margin-bottom: 10px; margin-top: 0px; padding-bottom: 20px;") do 
      %h1 Contact Us
      = link_to "email us: f2posrsranks@gmail.com", "mailto:f2posrsranks@gmail.com"
    
